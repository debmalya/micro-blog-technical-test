{% include 'header_without_menu.twig' %}        
 {% from 'form.html' import input as input_field %} 
 <form id="loginForm" method="POST" action="/api/formattedlogin/validate">
 <div align="center">
 <dl>
 	<dt>Username</dt>
	<dd>{{ input_field('user_name') }}</dd>
	<dt>Password</dt>
	<dd>{{ input_field('password', '', 'password') }}</dd> 
</dl>
</div>
<div align="center">
<a href="/api/userregistration/set">Register</a>
<input type="submit" value="Login"  class="w3-btn-block w3-ripple w3-teal"/>
</div>
</form>
{% include 'footer.twig' %}
